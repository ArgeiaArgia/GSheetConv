.custom-label {
    font-size: 20px;
    -unity-font-style: bold;
    color: rgb(68, 138, 255);
}

.valid-btn {
    transition-timing-function: ease-out-cubic;
    transition-duration: 0.25s;
}

.valid-btn.valid {
    background-color: rgb(68, 132, 60);
}

.valid-btn.invalid {
    background-color: rgb(132, 67, 67);
}

.add-btn {
}

.text-field {
    flex-grow: 1;
    overflow: hidden;
    text-overflow: ellipsis;
    flex-basis: 175px;
}
